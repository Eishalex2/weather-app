{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": ["airbnb-base", "prettier"],
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "no-console": "off", //removes errors for console.log messages
        "linebreak-style": ["error", "windows"] //solves lf vs. crlf issue
    }
}
